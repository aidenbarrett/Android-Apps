<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.itb.eng.components">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.itb.eng.componentCategories.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.itb.eng.componentCategories.CategoryDetailActivity" />

        <activity android:name="com.itb.eng.componentCategories.AddCategory"
            android:parentActivityName="com.itb.eng.componentCategories.MainActivity" />
    </application>

</manifest>